{
  "name": "apartment-app",
  "version": "1.0.0",
  "type": "module",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --tunnel",
    "dev": "expo start --tunnel --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "smoke-test": "node scripts/smoke-test.js",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --config jest.config.cjs",
    "test:unit": "jest --config jest.config.cjs --testPathPatterns=__tests__/unit --coverage",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --watchAll=false",
    "test:integration": "jest --config jest.config.cjs --testPathPatterns=__tests__/integration",
    "test:e2e": "detox test --config detox.config.cjs",
    "detox:build": "detox build --configuration ios.sim.debug",
    "detox:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@types/react": "19.2.7",
    "@types/react-native": "0.72.8",
    "expo": "~54.0.29",
    "expo-constants": "~18.0.12",
    "expo-file-system": "19.0.21",
    "expo-image-manipulator": "14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-print": "15.0.8",
    "expo-router": "5.1.10",
    "expo-sharing": "14.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-contacts": "8.0.7",
    "react-native-gesture-handler": "2.29.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "typescript": "5.9.3",
    "zustand": "5.0.9"
  },
  "private": true,
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@react-native/babel-preset": "^0.83.1",
    "@tanstack/react-query": "^5.90.12",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^11.0.15",
    "detox": "^20.46.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^7.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^19.1.0"
  }
}
